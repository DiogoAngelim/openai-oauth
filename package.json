    "@types/react": "18.2.67",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.2",
    "fastify": "^4.29.1",
    "jest": "^30.2.0",
    "lcov-result-merger": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "standard": "^17.1.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.50.1"
