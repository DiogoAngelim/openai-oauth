    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/common": "^11.1.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.10",
