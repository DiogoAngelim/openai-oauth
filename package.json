    "build": "pnpm --filter ./apps/frontend run build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/platform-express": "10.4.20",
    "@types/node": "25.0.3",
    "@types/pg": "^8.16.0",
    "express": "^5.2.1",
    "ioredis": "^5.8.2",
    "mysql2": "^3.16.0",
    "next": "^14.0.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "prom-client": "^15.1.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "ts-standard": "^12.0.2",
    "typescript": "^5.3.3",
    "winston": "^3.19.0"
