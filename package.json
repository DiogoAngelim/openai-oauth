{
  "name": "openai-oauth",
  "version": "1.0.0",
  "description": "Full stack monorepo for OpenAI OAuth integration.",
  "keywords": [
    "openai",
    "oauth",
    "monorepo",
    "nestjs",
    "nextjs",
    "typescript"
  ],
  "author": "Diogo <diogoangelim@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
