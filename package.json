{
  "pnpm": {
    "overrides": {
      "esbuild": ">=0.25.0"
    }
  },
  "name": "@openai-oauth/backend",
  "version": "1.0.0",
  "description": "Full stack monorepo for OpenAI OAuth integration.",
  "keywords": [
    "openai",
    "oauth",
    "monorepo",
    "nestjs",
    "nextjs",
    "typescript"
  ],
  "author": "Diogo <diogoangelim@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/DiogoAngelim/openai-oauth.git"
  },
  "homepage": "https://github.com/DiogoAngelim/openai-oauth#readme",
  "bugs": {
    "url": "https://github.com/DiogoAngelim/openai-oauth/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/DiogoAngelim"
  },
  "private": true,
  "type": "module",
  "main": "index.js",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "files": [
    "apps/",
    "packages/",
    "infra/",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "pnpm --filter @openai-oauth/backend test",
    "format": "npx prettier --write .",
    "typecheck": "tsc --noEmit",
    "start": "pnpm --filter ./apps/frontend dev",
    "deploy": "echo 'Add your deployment command here'"
