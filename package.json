    "@nestjs/testing": "^11.1.10",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "drizzle-kit": "0.31.8",
    "drizzle-orm": "0.45.1",
    "express": "^5.2.1",
    "fastify": "^5.6.2"
  }
